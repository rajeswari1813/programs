#include <stdio.h>
int main()
{
	float p;
	printf("Enter percentage");
	scanf("%f",&p);
	if (p>=70)
	    printf("distinction");
	else if (p>=60 && p<70)
	    printf("First class");
	else if (p>=50 && p<60)  
	    printf("second class");
	else if (p>=40 && p<50)
	    printf("Third class");
	else 
	    printf("Fail");
	return 0;
}
